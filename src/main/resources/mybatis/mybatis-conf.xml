<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- 批量设置别名(可以不配) 作用：就是在mapper.xml文件中直接写类名，也可以不用写全路径名。 -->
    <typeAliases>
        <typeAlias type="Integer" alias="java.lang.Integer" />
        <typeAlias type="Long" alias="java.lang.Long" />
        <typeAlias type="String" alias="java.lang.String" />
        <typeAlias type="Byte" alias="java.lang.Byte" />
        <typeAlias type="List" alias="java.util.List" />
        <!--User实体类-->
        <typeAlias type="User" alias="com.example.demo.entity.Test" />

        <package name="com.example.demo.entity" />
    </typeAliases>
    <!-- 【mybatis的核心配置文件】 -->
    <!-- 配置mybatis的分页插件PageHelper -->
    <!--<plugins>
        &lt;!&ndash; com.github.pagehelper为PageHelper类所在包名 &ndash;&gt;
        <plugin interceptor="com.github.pagehelper.PageHelper">
            &lt;!&ndash; 设置数据库类型Oracle,Mysql,MariaDB,SQLite,Hsqldb,PostgreSQL六种数据库 &ndash;&gt;
            <property name="dialect" value="mysql" />
        </plugin>
    </plugins>-->

    <!--设置数据库别名-->

</configuration>